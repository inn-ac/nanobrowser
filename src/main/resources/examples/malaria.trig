@prefix : <http://purl.org/nanobrowser/examples/malaria/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix pav: <http://swan.mindinformatics.org/ontologies/1.2/pav/> .
@prefix np: <http://www.nanopub.org/nschema#> .
@prefix npx: <http://krauthammerlab.med.yale.edu/nanopub/> .

:MalariaPub_Head {
    :MalariaPub a np:Nanopublication ;
        np:hasAssertion :MalariaPub_Assertion ;
        np:hasProvenance :MalariaPub_Provenance ;
        np:hasPublicationInfo :MalariaPub_Info .

    :MalariaPub_Assertion
        np:containsGraph :MalariaPub_Assertion_Head ;
        np:containsGraph :MalariaPub_Assertion_Statement .
}

:MalariaPub_Assertion_Head {
    :MalariaPub_Assertion
        npx:asSentence <http://purl.org/aida/Malaria+is+transmitted+by+mosquitoes.> ;
        npx:asFormula :MalariaPub_Assertion_Statement .
}

:MalariaPub_Assertion_Statement {
    <http://example.com/mosquito> <http://example.com/transmits> <http://example.com/malaria> .
}

:MalariaPub_Info {
    :MalariaPub
        pav:authoredBy <http://www.researcherid.com/rid/F-1227-2011> ;
        pav:createdBy <http://www.researcherid.com/rid/B-6810-2013> ;
        dc:created "2013-03-04T17:24:00Z"^^xsd:dateTime .
}
